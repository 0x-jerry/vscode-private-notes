{
  "name": "vscode-writing",
  "displayName": "vscode-writing",
  "description": "vscode writing tool",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "fantasy",
  "author": {
    "name": "Jerry Wang",
    "email": "x.jerry.wang@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/cwxyz007/vscode-writing/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cwxyz007/vscode-writing.git"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "grammars": [
      {
        "scopeName": "markdown.story",
        "path": "./syntaxes/markdown.story.tmLanguage.json",
        "injectTo": [
          "meta.paragraph.markdown",
          "text.html.markdown"
        ]
      }
    ],
    "commands": []
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "vscode:prepublish": "tsup src/extension.ts --sourcemap",
    "dev": "tsup src/extension.ts --sourcemap --watch",
    "version": "yarn changelog && git add CHANGELOG.md"
  },
  "peerDependencies": {
    "vscode": "*"
  },
  "devDependencies": {
    "@0x-jerry/eslint-config": "^0.1.3",
    "@types/node": "^17.0.1",
    "@types/vscode": "^1.63.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.5.0",
    "tsup": "^5.11.6",
    "typescript": "^4.5.4"
  }
}
